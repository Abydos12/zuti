{
  "productName": "zuti",
  "version": "0.0.0",
  "identifier": "fr.gbdx.zuti",
  "build": {
    "beforeDevCommand": "npm run vite:dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run vite:build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "Zuti",
        "center": true,
        "width": 800,
        "minWidth": 400,
        "height": 600,
        "minHeight": 200
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
